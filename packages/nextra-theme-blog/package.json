{
  "name": "nextra-theme-blog",
  "version": "2.0.0-beta.11",
  "description": "A Nextra theme for blogs.",
  "repository": "https://github.com/shuding/nextra",
  "author": "Shu Ding <g@shud.in>",
  "license": "MIT",
  "files": [
    "dist/*",
    "style.css"
  ],
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    "./style.css": "./style.css",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsup",
    "build:tailwind": "cross-env NODE_ENV=production pnpm postcss src/styles.css -o style.css --verbose",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit",
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\"",
    "dev:layout": "tsup --watch",
    "dev:tailwind": "cross-env TAILWIND_MODE=watch pnpm postcss src/styles.css -o style.css --watch",
    "prepublishOnly": "pnpm build",
    "clean": "rimraf ./dist ./style.css",
    "test": "vitest run",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@mdx-js/react": "^2.1.2",
    "next-themes": "^0.2.0-beta.2"
  },
  "peerDependencies": {
    "next": ">=9.5.3",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1",
    "react-cusdis": "^2.1.3"
  },
  "peerDependenciesMeta": {
    "react-cusdis": {
      "optional": true
    }
  },
  "devDependencies": {
    "nextra": "workspace:*",
    "next": ">=12",
    "vitest": "*",
    "react": "*",
    "react-dom": "*",
    "concurrently": "*",
    "cross-env": "*",
    "@types/react": "*",
    "@types/react-dom": "*",
    "tsup": "*",
    "postcss": "*",
    "postcss-cli": "*",
    "autoprefixer": "*",
    "cssnano": "*",
    "tailwindcss": "*",
    "@tailwindcss/nesting": "*",
    "@tailwindcss/typography": "*",
    "react-cusdis": "^2.1.3"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  }
}
