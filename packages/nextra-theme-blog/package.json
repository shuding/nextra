{
  "name": "nextra-theme-blog",
  "version": "3.2.5",
  "description": "A Nextra theme for blogs.",
  "repository": "https://github.com/shuding/nextra",
  "author": "Shu Ding <g@shud.in>",
  "license": "MIT",
  "exports": {
    "./style.css": "./dist/style.css",
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.mts"
    },
    "./cusdis": {
      "import": "./dist/cusdis.js",
      "types": "./dist/cusdis.d.mts"
    },
    "./tags": {
      "import": "./dist/tags.js",
      "types": "./dist/tags.d.mts"
    }
  },
  "types": "./dist/index.d.mts",
  "typesVersions": {
    "*": {
      "cusdis": [
        "./dist/cusdis.d.mts"
      ],
      "tags": [
        "./dist/tags.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:all": "pnpm build && pnpm build:tailwind",
    "build:tailwind": "pnpm postcss src/styles.css -o dist/style.css --verbose",
    "clean": "rimraf ./dist ./style.css",
    "dev": "concurrently \"pnpm dev:layout\" \"pnpm dev:tailwind\"",
    "dev:layout": "tsup --watch",
    "dev:tailwind": "TAILWIND_MODE=watch pnpm postcss src/styles.css -o dist/style.css --watch",
    "prepublishOnly": "pnpm build:all",
    "test": "vitest run",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit"
  },
  "peerDependencies": {
    "next": ">=13",
    "nextra": "workspace:*",
    "react": ">=18",
    "react-cusdis": "^2.1.3",
    "react-dom": ">=18"
  },
  "peerDependenciesMeta": {
    "react-cusdis": {
      "optional": true
    }
  },
  "dependencies": {
    "next-themes": "^0.4.0"
  },
  "devDependencies": {
    "@tailwindcss/nesting": "^0.0.0-insiders.565cd3e",
    "@tailwindcss/typography": "^0.5.9",
    "@types/react": "^18.2.23",
    "@types/react-dom": "^18.2.8",
    "concurrently": "^9.0.0",
    "next": "^15.0.0-rc.1",
    "nextra": "workspace:*",
    "postcss": "^8.4.33",
    "postcss-cli": "^11.0.0",
    "postcss-import": "^16.0.0",
    "postcss-lightningcss": "^1.0.0",
    "react": "^18.2.0",
    "react-cusdis": "^2.1.3",
    "react-dom": "^18.2.0",
    "tailwindcss": "^3.4.1",
    "vitest": "^2.0.3"
  },
  "sideEffects": false
}
