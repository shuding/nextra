{
  "name": "nextra-theme-docs",
  "version": "2.0.0-beta.4",
  "description": "A Nextra theme for documentation sites.",
  "main": "index.js",
  "module": "index.js",
  "repository": "https://github.com/shuding/nextra-theme-docs.git",
  "author": "Shu Ding <g@shud.in>",
  "license": "MIT",
  "files": [
    "dist/*",
    "index.js",
    "callout.js",
    "bleed.js",
    "style.css"
  ],
  "exports": {
    "./style.css": "./style.css",
    ".": {
      "import": "./dist/index.js"
    },
    "./callout": {
      "import": "./dist/callout.js"
    },
    "./bleed": {
      "import": "./dist/bleed.js"
    }
  },
  "scripts": {
    "dev": "yarn dev:layout & yarn dev:tailwind",
    "build": "yarn run build:tailwind && yarn run build:layout",
    "build:layout": "node scripts/build.js",
    "build:tailwind": "NODE_ENV=production yarn postcss src/styles.css -o style.css --verbose",
    "dev:layout": "node scripts/dev.js",
    "dev:tailwind": "TAILWIND_MODE=watch yarn postcss src/styles.css -o style.css -w --verbose",
    "prepublishOnly": "rm -rf dist && yarn build"
  },
  "dependencies": {
    "@mdx-js/react": "^2.0.0-rc.2",
    "@reach/skip-nav": "^0.11.2",
    "classnames": "^2.2.6",
    "focus-visible": "^5.1.0",
    "github-slugger": "^1.3.0",
    "grapheme-splitter": "^1.0.4",
    "intersection-observer": "^0.12.0",
    "match-sorter": "^4.2.0",
    "next-themes": "^0.0.8",
    "parse-git-url": "^1.0.1",
    "prism-react-renderer": "^1.1.1",
    "react-innertext": "^1.1.5",
    "title": "^3.4.2"
  },
  "peerDependencies": {
    "next": ">=9.5.3",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "devDependencies": {
    "autoprefixer": "^10.0.2",
    "cssnano": "^4.1.10",
    "esbuild": "^0.14.0",
    "postcss": "^8.2.1",
    "postcss-cli": "^8.3.0",
    "tailwindcss": "^2.2.4"
  }
}
