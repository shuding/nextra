{
  "name": "nextra",
  "version": "2.0.0-beta.21",
  "description": "Next.js and MDX based site generator.",
  "files": [
    "dist/*",
    "icons/*",
    "loader.js",
    "styles/*"
  ],
  "types": "./dist/types.d.ts",
  "main": "./dist/index.js",
  "typesVersions": {
    "*": {
      "compile": [
        "./dist/compile.d.ts"
      ],
      "context": [
        "./dist/context.d.ts"
      ],
      "data": [
        "./dist/ssg.d.ts"
      ],
      "ssg": [
        "./dist/ssg.d.ts"
      ],
      "locales": [
        "./dist/locales.d.ts"
      ],
      "icons": [
        "./dist/icons/index.d.ts"
      ],
      "components": [
        "./dist/components/index.d.ts"
      ]
    }
  },
  "exports": {
    "./package.json": "./package.json",
    ".": "./dist/index.js",
    "./data": {
      "import": "./dist/ssg.mjs",
      "types": "./dist/ssg.d.ts"
    },
    "./loader": "./loader.js",
    "./icons": {
      "import": "./dist/icons/index.mjs",
      "types": "./dist/icons/index.d.ts"
    },
    "./components": {
      "import": "./dist/components/index.mjs",
      "types": "./dist/components/index.d.ts"
    },
    "./*": {
      "import": "./dist/*.mjs",
      "types": "./dist/*.d.ts"
    }
  },
  "repository": "https://github.com/shuding/nextra",
  "license": "MIT",
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "types": "tsup --dts-only",
    "types:check": "tsc --noEmit",
    "test": "vitest run",
    "prepublishOnly": "pnpm build",
    "clean": "rimraf ./dist ./style.css",
    "format": "prettier --write ."
  },
  "dependencies": {
    "@mdx-js/mdx": "^2.1.2",
    "@napi-rs/simple-git": "^0.1.8",
    "github-slugger": "^1.4.0",
    "graceful-fs": "^4.2.10",
    "gray-matter": "^4.0.3",
    "react-children-utilities": "^2.8.0",
    "rehype-mdx-title": "^1.0.0",
    "rehype-pretty-code": "0.2.4",
    "remark-gfm": "^3.0.1",
    "remark-reading-time": "^2.0.1",
    "shiki": "0.10.1",
    "slash": "^3.0.0"
  },
  "peerDependencies": {
    "next": ">=9.5.3",
    "react": ">=16.13.1",
    "react-dom": ">=16.13.1"
  },
  "prettier": {
    "semi": false,
    "singleQuote": true,
    "trailingComma": "none",
    "arrowParens": "avoid"
  },
  "devDependencies": {
    "@types/github-slugger": "^1.3.0",
    "@types/graceful-fs": "^4.1.5",
    "@types/mdast": "^3.0.10",
    "@types/react": "^18.0.15",
    "@types/react-dom": "^18.0.6",
    "@types/webpack": "^5.28.0",
    "next": "^12.2.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "vitest": "^0.21.0"
  }
}
